{
    "/api/v2/hdrezka/id": {
        "get": {
            "description": "Search movie from HDRezka and get its id",
            "tags": [
                "HDRezka2"
            ],
            "parameters": [
                {
                    "in": "query",
                    "name": "search",
                    "required": true
                }
            ],
            "responses": {
                "200": {
                    "description": "OK"
                }
            }
        }
    },
    "/api/{version}/hdrezka/sources": {
        "post": {
            "description": "Get movie sources from HDRezka",
            "tags": [
                "HDRezka2"
            ],
            "parameters": [
                {
                    "in": "path",
                    "name": "version",
                    "description": "API Version",
                    "required": true,
                    "type": "string"
                },
                {
                    "in": "body",
                    "name": "Body",
                    "required": true,
                    "schema": {
                        "$ref": "#/definitions/getSourcesfromHDRezka"
                    }
                }
            ],
            "responses": {
                "200": {
                    "description": "OK"
                }
            }
        }
    },
    "/api/v2/hdrezka/imdb": {
        "get": {
            "description": "Get movie info from imdb",
            "tags": [
                "HDRezka2"
            ],
            "parameters": [
                {
                    "in": "query",
                    "name": "id",
                    "description": "IMDb id",
                    "required": true,
                    "type": "string"
                }
            ],
            "responses": {
                "200": {
                    "description": "OK"
                }
            }
        }
    },
    "/api/v2/hdrezka/available": {
        "get": {
            "description": "Get movie translation code and studio names from hdrezka",
            "tags": [
                "HDRezka2"
            ],
            "parameters": [
                {
                    "in": "query",
                    "name": "title",
                    "description": "Movie title",
                    "required": true,
                    "type": "string"
                }
            ],
            "responses": {
                "200": {
                    "description": "OK"
                }
            }
        }
    }
}